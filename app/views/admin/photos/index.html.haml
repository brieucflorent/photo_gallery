-#  This file is app/views/movies/index.html.haml
%h1 All Photos

  
%table#photos
  %thead
    %tr
      %th title
      %th imagfile
      %th More Info
  %tbody
    - @photos.each do |photo|
      %tr
        %td= photo.title  
        %td= image_tag photo.imagefile_url(:thumb)
        %td= link_to "More about #{photo.title}", edit_admin_photo_path(photo)